{
  "author": "Alexandre Masy <hello@alexandremasy.com",
  "description": "Customize the body from the GingerView instance",
  "devDependencies": {
    "rollup": "^2.21.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0"
  },
  "files": [
    "dist/*.js"
  ],
  "keywords": [
    "spices",
    "ginger",
    "ginger:plugin",
    "body"
  ],
  "license": "MIT",
  "main": "dist/spices-ginger-body.min.js",
