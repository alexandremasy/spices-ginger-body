{
  "author": "Alexandre Masy <hello@alexandremasy.com",
  "description": "Customize the body from the GingerView instance",
  "devDependencies": {
    "rollup": "^2.21.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0"
  },
  "files": [
    "dist/*.js"
  ],
  "keywords": [
    "spices",
    "ginger",
    "ginger:plugin",
    "body"
  ],
  "license": "MIT",
  "main": "dist/spices-ginger-body.min.js",
  "name": "@ginger/body",
  "nodemonConfig": {
    "ignore": [
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "ext": "js"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.infinity-commerce.io/"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/infinity-mobile/ginger-body.git"
  },
  "scripts": {
    "bundle": "rollup -c",
    "start": "nodemon --exec \"yarn bundle\""
  },
  "version": "1.0.2"
}
